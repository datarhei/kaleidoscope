{
  "name": "@thiagopnts/kaleidoscope",
  "version": "1.1.1",
  "description": "360ยบ video/image viewer",
  "main": "dist/kaleidoscope.min.js",
  "module": "dist/kaleidoscope.es.js",
  "scripts": {
    "test": "karma start --single-run",
    "build": "rollup -c",
    "build-release": "BABEL_ENV=production rollup -c && babel dist/kaleidoscope.js --presets minify > dist/kaleidoscope.min.js && babel dist/kaleidoscope.iife.js --presets minify > dist/kaleidoscope.iife.min.js"
  },
  "files": [
    "package.json",
    "LICENSE",
    "README.md",
    "dist/kaleidoscope.es.js",
    "dist/kaleidoscope.iife.min.js",
    "dist/kaleidoscope.min.js",
    "src",
    "test"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "auto"
        }
      ],
      "minify"
    ],
    "env": {
      "production": {
        "presets": [
          "minify"
        ]
      }
    },
    "plugins": []
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiagopnts/kaleidoscope.git"
  },
  "keywords": [
    "360",
    "video",
    "video360",
    "360video",
    "360image",
    "image360",
    "image"
  ],
  "author": "Thiago Pontes <hello@thiago.me>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/thiagopnts/kaleidoscope/issues"
  },
  "homepage": "https://github.com/thiagopnts/kaleidoscope#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/preset-env": "^7.9.6",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "babel-preset-minify": "^0.5.1",
    "chai": "^4.2.0",
    "jasmine-core": "^3.3.0",
    "karma": "^3.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-rollup-preprocessor": "^7.0.5",
    "karma-sinon": "^1.0.5",
    "mocha": "^5.2.0",
    "rollup": "^2.7.5",
    "sinon": "^7.1.1",
    "uglify-js": "^3.9.1"
  },
  "dependencies": {
    "three": "0.115.0"
  }
}
